{
    "version": "0.2.0",
    "configurations": [
        
      {
        "name": "Debug TypeScript (ts-node)",
        "type": "pwa-node",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "runtimeArgs": [
          "-r",
          "ts-node/register/transpile-only"
        ],
        "args": [
          "${workspaceFolder}/src/server.ts"
        ],
        "envFile": "${workspaceFolder}/.env",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }
  